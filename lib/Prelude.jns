-- Identity function
let 1 id = (\x. \y. y : (0 x : 𝘜) -> (1 y : x) -> x)

-- Projection to the first element of the multiplicative pair in the erased part of the theory
let w proj1 = (λx y z. let c @ a, b = z in a : x)
              : ∀ (0 x : U)
                  (0 y : (0 a : x) -> U)
                  (w z : (w a : x) * y a)
                . x

-- Projection to the second element of the multiplicative pair in the erased part of the theory
let w proj2 = (λx y z. let c @ a, b = z in b : y (let f @ d, e = c in d : x))
              : ∀ (0 x : U)
                  (0 y : (0 a : x) -> U)
                  (w z : (w a : x) * y a)
                . y (let c @ a, b = z in a : x)

-- The exponential type
let 0 ofcW = (λx. (w y : x) * I) : (0 x : U) -> U

-- Eliminator for values of the exponential type
let w ofcElim = (λx y z a. let 1 d @ b, c = z in let 1 d @ () = c in a b : y z : y z)
                : ∀ (0 x : U)
                    (0 y : (0 b : (w b : x) * I) -> U)
                    (1 z : (w b : x) * I)
                    (1 a : (w b : x) -> y z)
                  . y z

